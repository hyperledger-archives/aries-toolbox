{
  "name": "aries-toolbox",
  "version": "0.0.1",
  "author": "Sam Curren <telegramsam@gmail.com>",
  "description": "Tools for working with Aries Agents",
  "repository": {
    "type": "https",
    "url": "https://github.com/hyperledger/aries-toolbox.git"
  },
  "license": "Apache-2.0",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder --publish never",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": ""
  },
  "build": {
    "productName": "aries-toolbox",
    "appId": "com.example.aries-toolbox",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "@chenfengyuan/vue-qrcode": "^1.0.1",
    "axios": "^0.18.0",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.1.0",
    "dompurify": "^2.0.8",
    "element-ui": "^2.10.1",
    "encryption-envelope-js": "1.1.4",
    "marked": "^0.8.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "tslib": "^1.10.0",
    "uuid": "^3.3.3",
    "v-jsoneditor": "^1.0.13",
    "vue": "^2.5.16",
    "vue-electron": "^1.0.6",
    "vue-json-pretty": "^1.6.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-electron": "^1.0.0",
    "websocket-as-promised": "^1.0.1",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "ajv": "^6.5.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^2.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^5.1.6",
    "css-loader": "^3.4.2",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^7.2.4",
    "electron-builder": "^21.2.0",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^14.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "0.4.0",
    "multispinner": "git+https://github.com/codekirei/node-multispinner.git#ad77a3aadb55dae414e42d79e5bdba7d919b5dad",
    "node-loader": "^0.6.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  },
  "optionalDependencies": {
    "jquery": "1.9.1 - 3",
    "popper.js": "^1.14.7"
  }
}
